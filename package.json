{
  "name": "uts",
  "version": "1.1.3",
  "description": "Microscopic time series database for Node and the browser",
  "main": "dist/uts.js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "rimraf dist && tsc",
    "watch": "rimraf dist && tsc --watch",
    "test": "npm run test:unit && npm run test:lint && npm run test:fmt",
    "test:unit": "mocha \"dist/**/*.test.js\"",
    "test:lint": "eslint \"src/**/*.ts\"",
    "test:fmt": "prettier --list-different \"src/**/*.ts\"",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test:lint -- --fix",
    "build": "tsc && tsc -d",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/uts.git"
  },
  "keywords": [
    "time",
    "series",
    "database",
    "browser"
  ],
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/uts/issues"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "arrowParens": "avoid"
  },
  "homepage": "https://github.com/microsoft/uts#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-plugin-header": "^3.1.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "typescript": "^4.0.2"
  }
}
